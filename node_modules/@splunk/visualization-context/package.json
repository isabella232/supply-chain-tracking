{
  "name": "@splunk/visualization-context",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.5.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build:ci": "yarn build && yarn docs",
    "clean": "yarn clean:webpack && yarn clean:test-reports",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "lint": "eslint src --ext \".js,.jsx\"",
    "lint:ci": "eslint src --ext \".js,.jsx\" -f junit -o test-reports/lint-results.xml",
    "lint:fix": "eslint src --ext \".js,.jsx\" --fix",
    "test:unit": "jest src/.*",
    "test:unit:ci": "yarn run test:unit",
    "test:unit:watch": "jest --watch src/.*"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.0",
    "@splunk/ui-utils": "^1.2.1",
    "uuid-v4": "^0.1.0"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/jsdoc-loader": "^3.0.0",
    "@splunk/visualization-build-tools": "^23.5.0",
    "@splunk/webpack-configs": "^4.0.0",
    "babel-jest": "^26.0.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.0.0",
    "jest-localstorage-mock": "^2.4.2",
    "raw-loader": "^4.0.0",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.2.1"
  }
}
