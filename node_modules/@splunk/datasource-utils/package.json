{
  "name": "@splunk/datasource-utils",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.3.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build:ci": "yarn build && yarn docs",
    "clean": "rm -rf ./build && rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "docs": "NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:publish": "cicd-publish-docs build/docs/internal --force",
    "docs:start": "webpack-dev-server --watch --config webpack.docs.config.js",
    "lint": "eslint src --ext \".js,.jsx\"",
    "test": "yarn run test:unit",
    "test:ci": "yarn run test:unit:ci",
    "test:unit": "jest src/.*",
    "test:unit:ci": "yarn test:unit --testPathIgnorePatterns='flaky.test' --ci",
    "test:unit:watch": "yarn run test:unit --watchAll"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.15.4",
    "@splunk/dashboard-utils": "^23.3.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@splunk/cicd-tools": "^0.4.0",
    "@splunk/dashboard-build-tools": "23.3.0",
    "@splunk/dashboard-definition": "^23.3.0",
    "@splunk/dashboard-types": "^23.3.0",
    "@splunk/jsdoc-loader": "^3.0.0",
    "@splunk/react-docs": "^0.11.0",
    "jest": "^27.1.0",
    "query-string": "^6.13.8",
    "raw-loader": "^4.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.1",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  },
  "types": "types"
}
