{
  "name": "@splunk/react-visualizations",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.5.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build-storybook": "build-storybook -c .storybook -s fonts -o .storybook-bundle",
    "build:ci": "yarn build && yarn docs",
    "clean": "yarn clean:storybook && yarn clean:webpack && yarn clean:test-reports",
    "clean:storybook": "rm -rf .storybook-bundle",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm && rm -rf ./common ./utils",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "docs": "NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:publish": "cicd-publish-docs build/docs --force",
    "docs:start": "webpack-dev-server --watch --config webpack.docs.config.js  --open 'Google Chrome'",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:ci": "eslint src --ext \".js,.jsx,.ts,.tsx\" -f junit -o test-reports/lint-results.xml",
    "lint:fix": "yarn run lint:js --fix && yarn run lint:ts --fix",
    "lint:js": "eslint src --ext \".js,.jsx,.es\"",
    "lint:ts": "eslint src --ext \".ts,.tsx\" --config ../../.tslintrc.js --no-eslintrc",
    "publish:ci": "cicd-publish-package && cicd-publish-docs",
    "storybook": "start-storybook -p 6006 -c .storybook -s fonts",
    "test": "yarn run test:unit && yarn run test:visual",
    "test:ci": "yarn run test:unit:ci && yarn run test:visual:ci",
    "test:unit": "jest src/.*",
    "test:unit:ci": "yarn run test:unit",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:visual": "yarn build-storybook --quiet && yarn loki test --reactUri file:./.storybook-bundle",
    "test:visual:update": "yarn loki approve",
    "test:visual:ci": "yarn test:visual --requireReference --verboseRenderer"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.0",
    "@splunk/charting-bundle": "^23.5.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/ui-utils": "^1.2.1",
    "@splunk/visualization-color-palettes": "^23.5.0",
    "@splunk/visualization-context": "^23.5.0",
    "@splunk/visualization-themes": "^23.5.0",
    "@splunk/visualizations": "^23.5.0",
    "@splunk/visualizations-shared": "^23.5.0",
    "@turf/bbox-clip": "^6.0.3",
    "@turf/helpers": "^6.1.4",
    "chroma-js": "^2.0.0",
    "d3-scale": "^3.1.0",
    "jexl": "^2.2.2",
    "lodash": "^4.17.4",
    "react-event-listener": "^0.6.4",
    "xpath": "^0.0.32"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.0",
    "@splunk/cicd-tools": "^0.4.0",
    "@splunk/docs-gen": "^0.11.1",
    "@splunk/jsdoc-loader": "^3.0.0",
    "@splunk/react-docs": "^0.11.0",
    "@splunk/react-ui": "^4.0.3",
    "@splunk/themes": "^0.8.0",
    "@splunk/visualization-build-tools": "^23.5.0",
    "@splunk/webpack-configs": "^4.0.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.152",
    "@types/react": "^16.7.0",
    "babel-jest": "^26.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "docgen-loader": "^1.3.4",
    "jest": "^26.0.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-styled-components": "^7.0.3",
    "loki": "^0.28.1",
    "query-string": "^6.14.1",
    "raf": "^3.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "16.11.0",
    "styled-components": "^5.2.1",
    "text-encoding": "^0.7.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
