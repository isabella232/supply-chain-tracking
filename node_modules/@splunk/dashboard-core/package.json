    "@splunk/dashboard-ui": "^23.3.0",
    "@splunk/dashboard-utils": "^23.3.0",
    "@splunk/datasource-utils": "^23.3.0",
    "@splunk/datasources": "^23.3.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/react-icons": "^3.1.0",
    "@splunk/react-ui": "^4.1.0",
    "@splunk/themes": "^0.9.0",
    "@splunk/ui-utils": "^1.4.0",
    "@splunk/visualization-encoding": "^23.5.0",
    "hash-it": "^5.0.0",
    "lodash": "^4.17.21",
    "memoize-one": "^5.2.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react-composer": "^5.0.1",
    "react-full-screen": "^1.0.2",
    "react-redux": "^7.2.2",
    "react-sortable-hoc": "^1.11.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "rxjs": "^5.5.12"
