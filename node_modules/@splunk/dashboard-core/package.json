    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "enzyme": "^3.10.0",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "loki": "^0.28.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
