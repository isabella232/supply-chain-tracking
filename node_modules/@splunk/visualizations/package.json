{
  "name": "@splunk/visualizations",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.5.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build-storybook": "build-storybook -c .storybook -s fonts -o .storybook-bundle",
    "build:ci": "yarn build && yarn docs",
    "clean": "yarn clean:storybook && yarn clean:webpack && yarn clean:test-reports",
    "clean:storybook": "rm -rf .storybook-bundle",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "docs": "INTERNAL=true NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:external": "INTERNAL=false NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:publish": "cicd-publish-docs build/docs --force",
    "docs:publish:external": "../build-tools/createPublicPackage.js && cicd-publish-docs build/docs-external --force && ../build-tools/revertPublicPackage.js",
    "docs:start": "INTERNAL=true webpack-dev-server --watch --config webpack.docs.config.js  --open 'Google Chrome'",
    "docs:start:external": "INTERNAL=false webpack-dev-server --watch --config webpack.docs.config.js  --open 'Google Chrome'",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:ci": "eslint src --ext \".js,.jsx,.ts,.tsx\" -f junit -o test-reports/lint-results.xml",
    "lint:fix": "yarn run lint:js --fix && yarn run lint:ts --fix",
    "lint:js": "eslint src --ext \".js,.jsx,.es\"",
    "lint:ts": "eslint src --ext \".ts,.tsx\" --config ../../.tslintrc.js --no-eslintrc",
    "publish:ci": "cicd-publish-package && cicd-publish-docs",
    "storybook": "start-storybook -p 6006 -c .storybook -s fonts",
    "test": "yarn run test:unit && yarn run test:visual",
    "test:ci": "yarn run test:unit:ci && yarn run test:visual:ci",
    "test:unit": "jest src/.* .storybook/.*",
    "test:unit:ci": "yarn run test:unit",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:visual": "../../tools/visual.test.sh -p visualizations",
    "test:visual:ci": "yarn run build-storybook && jest ./tests/.*",
    "test:visual:update": "yarn test:visual --update"
  },
