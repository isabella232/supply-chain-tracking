{
  "name": "@splunk/dashboard-ui",
  "description": "Dashboard internal shared ui widgets",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.3.0",
  "scripts": {
    "create-env": "echo \"export default { version: '$(jq -r '.version' package.json)' };\" > 'src/utils/environment.ts'",
    "prebuild": "yarn create-env",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "prebuild-storybook": "yarn create-env",
    "build-storybook": "build-storybook -c .storybook -o .storybook-bundle",
    "clean": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "predev": "yarn create-env",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "yarn run test:unit",
    "test:ci": "yarn run test:unit:ci",
    "pretest:unit": "yarn create-env",
    "test:unit": "jest src/*",
    "test:unit:watch": "yarn run test:unit --watchAll",
    "test:unit:ci": "yarn test:unit --testPathIgnorePatterns='flaky.test' --ci",
    "test:unit:ci:flaky": "yarn test:unit --testPathIgnorePatterns='src/((?!flaky.test).)*$' --ci --config=jest.flaky.config.js",
    "pretest:visual": "yarn create-env",
    "test:visual": "yarn build-storybook --quiet && yarn loki test --reactUri file:./.storybook-bundle",
    "test:visual:update": "yarn loki approve",
    "test:visual:ci": "yarn test:visual --requireReference --verboseRenderer"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.15.4",
    "@splunk/dashboard-icons": "^23.3.0",
    "@splunk/dashboard-telemetry": "^23.3.0",
    "@splunk/dashboard-utils": "^23.3.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/react-icons": "^3.1.0",
    "@splunk/react-search": "^4.0.1",
    "@splunk/react-ui": "^4.1.0",
    "@splunk/splunk-utils": "^2.0.0",
    "@splunk/themes": "^0.9.0",
    "@splunk/ui-utils": "^1.4.0",
    "@splunk/visualizations-shared": "^23.5.0",
    "@testing-library/user-event": "^13.2.1",
    "lodash": "^4.17.21",
    "memoize-one": "^5.2.1",
    "prop-types": "^15.7.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "@splunk/dashboard-context": "^23.3.0",
    "@splunk/visualization-context": "^23.5.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@splunk/dashboard-build-tools": "23.3.0",
    "@splunk/dashboard-context": "^23.3.0",
    "@splunk/dashboard-types": "^23.3.0",
    "@splunk/datasource-utils": "^23.3.0",
    "@splunk/themes": "^0.9.0",
    "@splunk/visualization-context": "^23.5.0",
    "@storybook/react": "^6.3.7",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/react-redux": "^7.1.18",
    "enzyme": "^3.10.0",
    "jest": "^27.1.0",
    "jest-styled-components": "^7.0.5",
    "loki": "^0.28.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.3.1",
    "testing-library-selector": "^0.2.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.1",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  },
  "types": "./types"
}
