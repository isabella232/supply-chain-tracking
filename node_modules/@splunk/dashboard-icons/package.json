{
  "name": "@splunk/dashboard-icons",
  "description": "Dashboard internal shared ui widgets",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.3.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build-storybook": "build-storybook -c .storybook -o .storybook-bundle",
    "clean": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "yarn run test:unit",
    "test:ci": "yarn run test:unit:ci",
    "test:unit": "jest src/.* --passWithNoTests",
    "test:unit:ci": "yarn test:unit --testPathIgnorePatterns='flaky.test' --ci",
    "test:unit:watch": "yarn run test:unit --watchAll",
    "test:visual": "yarn build-storybook --quiet && yarn loki test --reactUri file:./.storybook-bundle",
    "test:visual:update": "yarn loki approve",
    "test:visual:ci": "yarn test:visual --requireReference --verboseRenderer"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.15.4",
    "@splunk/dashboard-utils": "^23.3.0",
    "@splunk/react-icons": "^3.1.0",
    "@splunk/ui-utils": "^1.4.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.4",
    "@splunk/dashboard-build-tools": "23.3.0",
    "@splunk/themes": "^0.9.0",
    "@storybook/react": "^6.3.7",
    "jest": "^27.1.0",
    "loki": "^0.28.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "16.14.0",
    "storybook-addon-matrix": "^1.6.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.1",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  },
  "types": "types"
}
