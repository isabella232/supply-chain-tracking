{
  "name": "@splunk/visualization-migrations",
  "description": "Migration scripts for visualization definitions",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.5.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "clean": "yarn clean:webpack && yarn clean:test-reports",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "test": "yarn run test:unit",
    "test:unit": "jest --passWithNoTests",
    "test:unit:ci": "yarn run test:unit -- --ci"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.0",
    "@splunk/visualization-encoding-parsers": "^23.5.0",
    "@splunk/visualizations": "^23.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.0",
    "@splunk/dashboard-types": "^23.0.0",
    "@splunk/visualization-build-tools": "^23.5.0",
    "babel-jest": "^26.0.0",
    "jest": "^26.0.0"
  },
  "main": "./index.js",
  "types": "./types"
}
