{
  "name": "@splunk/visualizations-shared",
  "description": "Visualizations Shared Files",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.5.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build-storybook": "build-storybook -c .storybook -s fonts -o .storybook-bundle",
    "clean": "yarn clean:storybook && yarn clean:webpack && yarn clean:test-reports",
    "clean:storybook": "rm -rf .storybook-bundle",
    "clean:webpack": "git ls-files --others -i --exclude-standard *.js *.map | xargs rm",
    "clean:test-reports": "rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "yarn run lint:js && yarn run lint:ts",
    "lint:fix": "yarn run lint:js --fix && yarn run lint:ts --fix",
    "lint:js": "eslint src --ext \".js,.jsx,.es\"",
    "lint:ts": "eslint . --ext \".ts,.tsx\" --config ../../.tslintrc.js --no-eslintrc",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:ci": "yarn run test:unit:ci && yarn run test:integration:ci",
    "test:integration": "echo integration test",
    "test:integration:ci": "echo integration test ci",
    "test:unit": "jest src/.*",
    "test:unit:ci": "yarn run test:unit",
    "test:unit:watch": "jest --watch src/.*",
    "test:visual": "../../tools/visual.test.sh -p visualizations-shared",
    "test:visual:ci": "yarn run build-storybook && JEST_JUNIT_OUTPUT_NAME='./junit.visual.xml' jest tests/.* --config=jest.visual.config.js",
    "test:visual:update": "yarn test:visual --update"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/react-icons": "^3.0.1",
    "@splunk/react-ui": "^4.0.3",
    "@splunk/themes": "^0.8.0",
    "@splunk/ui-utils": "^1.2.1",
    "element-resize-detector": "^1.1.14",
    "lodash": "^4.17.4",
    "numeral": "^2.0.6",
    "resize-observer-polyfill": "^1.5.1",
    "xmlserializer": "^0.6.1"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.0",
    "@splunk/react-docs": "^0.11.0",
    "@splunk/visualization-build-tools": "^23.5.0",
    "@splunk/visualization-context": "^23.5.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-storyshots": "^6.1.20",
    "@storybook/addon-storyshots-puppeteer": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "babel-jest": "^26.0.0",
    "jest": "^26.0.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-image-snapshot": "^4.0.0",
    "jest-junit": "^12.0.0",
    "jest-styled-components": "^7.0.3",
    "raf": "^3.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.1",
    "text-encoding": "^0.7.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.1.2",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  }
}
